                            @if (Model.UserInfo != null)
                            {
                                <a asp-controller="Account" asp-action="UserDetail" style="color:#777b81"><i class="fa fa-user"></i> </a>
                                <div class="header__top__right__social">
                                </div>
                                <a asp-controller="Account" asp-action="Logout" style="color:#777b81">  <img width="40" src="https://img.icons8.com/external-vitaliy-gorbachev-lineal-vitaly-gorbachev/60/000000/external-exit-emergency-vitaliy-gorbachev-lineal-vitaly-gorbachev.png" /></a>
                            }
                            else
                            {
                                <a asp-controller="Account" asp-action="Login" style="color:#777b81"><i class="fa fa-user"></i> Giriş Yap</a>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
