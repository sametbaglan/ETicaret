<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Humberger Begin -->
    <div class="humberger__menu__overlay"></div>
    <div class="humberger__menu__wrapper">
        <div class="humberger__menu__logo">
            <a href="#"><img src="~/logos/allllll 2.jpg" alt=""></a>
        </div>
        <div class="humberger__menu__cart">
            <ul>
                <li><a href="#"><i class="fa fa-heart"></i> <span>1</span></a></li>
                <li><a asp-controller="Checkout" asp-action="Checkout"><i class="fa fa-shopping-bag"></i> <span></span></a></li>
            </ul>
            @*  <div class="header__cart__price">Sepet: <span></span></div>*@
        </div>
        <div class="humberger__menu__widget">

            <div class="header__top__right__auth">
                @if (Model.UserInfo == null)
                {
                    <a asp-controller="Account" asp-action="Login" style="color:#777b81"><i class="fa fa-user"></i> Giriş Yap</a>
                }
                else
                {
                    <a asp-controller="Account" asp-action="UserDetail" style="color:#777b81"><i class="fa fa-user"></i> </a>
                    <div class="header__top__right__social">
                    </div>
                    <a asp-controller="Account" asp-action="Logout" style="color:#777b81">  <img width="40" src="https://img.icons8.com/external-vitaliy-gorbachev-lineal-vitaly-gorbachev/60/000000/external-exit-emergency-vitaliy-gorbachev-lineal-vitaly-gorbachev.png" /></a>
                }
            </div>
        </div>
        <nav class="humberger__menu__nav mobile-menu">
            <ul>
                @if (Model.UserInfo != null)
                {
                    @if (Model.UserInfo.role == "A")
                    {
                        <li class=""><a asp-controller="Product" asp-action="ProductList">Ana Sayfa</a></li>
                        <li>
                            <a asp-controller="Admin" asp-action="Ürünler">Ürün Ekleme</a>
                            <ul class="header__menu__dropdown">
                                <li><a asp-controller="Admin" asp-action="Create"> Ürün Ekle</a></li>
                                <li><a asp-controller="Admin" asp-action="List">Ürünler</a></li>
                            </ul>
                        </li>
                        <li>
                            <a asp-controller="Categoria" asp-action="GetAll">Kategori </a>
                            <ul class="header__menu__dropdown">
                                <li><a asp-controller="Categoria" asp-action="CategoriaAdd"> Kategori Ekle</a></li>
                                <li><a asp-controller="Categoria" asp-action="GetAll">Kategoriler</a></li>
                            </ul>
                        </li>
                        <li>
                            <a asp-controller="Varyant" asp-action="Attirbutes">Varyant </a>
                            <ul class="header__menu__dropdown">
                                <li><a asp-controller="Varyant" asp-action="BodySizeAdd">Beden Ekle</a></li>
                                <li><a asp-controller="Varyant" asp-action="ColorAdd">Renk Ekle</a></li>
                                <li><a asp-controller="Varyant" asp-action="Attirbutes">Varyantlar</a></li>
                            </ul>
                        </li>
                        <li>
                            <a asp-controller="Admin" asp-action="MyNewOrders">Siparişlerim  <span></span></a>
                            <ul class="header__menu__dropdown">
                                <li><a asp-controller="Admin" asp-action="PacketList">Kargodakiler</a></li>
                                <li><a asp-controller="Admin" asp-action="Remainder">Genel Bakiye</a></li>
                            </ul>
                        </li>
                    }
                    else
                    {
                        <li class=""><a asp-controller="Product" asp-action="ProductList">Ana Sayfa</a></li>
                        <li>
                            <a>Sepettekiler</a>
                            <ul class="header__menu__dropdown">
                                <li><a asp-controller="Checkout" asp-action="Checkout">Ödeme</a></li>
                            </ul>
                        </li>
                    }
                }
                else
                {
                    <li class=""><a asp-controller="Product" asp-action="ProductList">Ana Sayfa</a></li>

                }
            </ul>
        </nav>
        <div id="mobile-menu-wrap"></div>
        <div class="header__top__right__social">
            <a href="#"><i class="fa fa-facebook"></i></a>
            <a href="#"><i class="fa fa-twitter"></i></a>
            <a href="#"><i class="fa fa-linkedin"></i></a>
            <a href="#"><i class="fa fa-pinterest-p"></i></a>
        </div>
        <div class="humberger__menu__contact">
            <ul>
                <li><i class="fa fa-envelope"></i> support@giyimover.com</li>
                <li>100 Üzeri Kargo Bedava</li>
            </ul>
        </div>
    </div>
