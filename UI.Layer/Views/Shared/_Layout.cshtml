                                    <li>
                                        <a asp-controller="Checkout" asp-action="Checkout">Sepettekiler</a>
                                        <ul class="header__menu__dropdown">
                                            <li><a asp-controller="Checkout" asp-action="Checkout">Ödeme</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a asp-controller="Order" asp-action="MyOrders">Siparişlerim</a>
                                    </li>
                                }
                            }
                            else
                            {
                                  <li class=""><a asp-controller="Product" asp-action="ProductList">Ana Sayfa</a></li>
                                   
                            }
                        </ul>
                    </nav>
                </div>
                <div class="col-lg-3">
                    <div class="header__cart">
                        <ul>
                            <!-- <li><a href="#"><i class="fa fa-heart"></i> <span>1</span></a></li> -->
                            <!-- <li><a href="#"><i class="fa fa-shopping-bag"></i> <span>3</span></a></li> -->
                        </ul>
                        @* @if (Model.UserInfo == null || Model.UserInfo.role == "B")
                            {
                            @if (Model.fiyat != null)
                            {
                            <div class="header__cart__price">Sepet : <span>@Model.fiyat TL</span></div>
                            }
                            else
                            {
                            <div class="header__cart__price">Sepet : <span>0 TL</span></div>
                            }
                            }*@
                    </div>
                </div>
            </div>
            <div class="humberger__open">
                <i class="fa fa-bars"></i>
            </div>
        </div>
    </header>
    <!-- Header Section End -->
    <!-- Hero Section Begin -->
    @if (Model.UserInfo == null || Model.UserInfo.role == "B")
    {
        <section class="hero">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3">

                        <div class="hero__categories">

                            <div class="hero__categories__all">
                                <i class="fa fa-bars"></i>

                                <span>Kategoriler</span>
                            </div>
                            <ul>
                                @foreach (var item in Model.Categorias)
                                {
                                    <li><a href="/Product/GetCategoryByProductList/@item.CategoriaID">@item.CategoryName</a></li>
                                }
                            </ul>

                        </div>

                    </div>

                    <div class="col-lg-9">

                        <div class="hero__search">
                            @if (Model.UserInfo == null || Model.UserInfo.role == "B")
                            {
                                @*         <div class="hero__search__form">
                                    <form action="#">
                                    <div class="hero__search__categories">
                                    Kategori1
                                    <span class="arrow_carrot-down"></span>
                                    </div>
                                    <input type="text" placeholder="Ürünü Aramak İster misiniz ?">
                                    <button type="submit" class="site-btn">Getir</button>
                                    </form>
                                    </div>*@
                                @*   <div class="hero__search__phone">
                                    <div class="hero__search__phone__icon">
                                    <i class="fa fa-phone"></i>
                                    </div>
                                    <div class="hero__search__phone__text">
                                    <h5>Telefon Numarası</h5>
                                    <span>Destek 24/7 Zaman</span>
                                    </div>
                                    </div>*@
                            }
                            @RenderBody()
                        </div>

                    </div>

                </div>

            </div>
        </section>
    }
    @RenderBody()
    <section class="categories">
        <div class="container">
            <div class="row">
                <div class="categories__slider owl-carousel">
                    <div class="col-lg-3">
                        <!-- <div class="categories__item set-bg" data-setbg="img/categories/cat-1.jpg">
                            <h5><a href="#">Fresh Fruit</a></h5>
                        </div> -->
                    </div>

                </div>
            </div>
        </div>
