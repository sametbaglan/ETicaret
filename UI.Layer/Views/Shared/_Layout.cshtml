    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css2?family=Cairo:wght@200;300;400;600;900&amp;display=swap" rel="stylesheet">
    <style>
        .hero__search__form form button {
            position: absolute;
            right: 0;
            top: -1px;
            background: #252525;
            height: 50px;
        }

        .hero__categories__all {
            background: #252525 !important;
        }

        .hero__item {
            height: 431px;
