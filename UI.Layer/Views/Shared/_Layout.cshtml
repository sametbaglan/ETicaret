                                            <li><a asp-controller="Varyant" asp-action="BodySizeAdd">Beden Ekle</a></li>
                                            <li><a asp-controller="Varyant" asp-action="ColorAdd">Renk Ekle</a></li>
                                            <li><a asp-controller="Varyant" asp-action="Attirbutes">Varyantlar</a></li>
                                        </ul>
                                    </li>

                                    <li>
                                        <a asp-controller="Trendyol" asp-action="ShipmenList">Trendyol</a>
                                        <ul class="header__menu__dropdown">
                                            <li><a asp-controller="Trendyol" asp-action="ProductSend">Ürün Gönderme</a></li>
                                        </ul>
                                    </li>
                                    <li>
