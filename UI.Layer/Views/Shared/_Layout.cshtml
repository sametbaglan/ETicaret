                            {
                                @if (Model.UserInfo.role == "A")
                                {
                                    <li class=""><a asp-controller="Product" asp-action="ProductList">Ana Sayfa</a></li>
                                    <li>
                                        <a asp-controller="Admin" asp-action="List">Ürün Ekleme</a>
                                        <ul class="header__menu__dropdown">
                                            <li><a asp-controller="Admin" asp-action="Create"> Ürün Ekle</a></li>
                                            <li><a asp-controller="Admin" asp-action="List">Ürünler</a></li>
                                        </ul>
